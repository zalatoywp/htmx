{
  "version": 3,
  "routes": [
    {
      "src": "^/_astro/(.*)$",
      "headers": {
        "mode": "no-cors",
        "access-control-allow-origin": "*",
        "content-type": "application/json",
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "handle": "filesystem"
    }
  ]
}
