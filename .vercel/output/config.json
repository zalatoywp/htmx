{
  "version": 3,
  "routes": [
    {
      "src": "^/_astro/(.*)$",
      "Accept": "*/*",
      "headers": {
        "mode": "no-cors",
        "method": "GET",
        "cache-control": "no-cache",
        "Content-Type": "application/json , charset=utf-8",
        "Access-Control-Allow-Origin": "*"
      },
      "continue": true
    },
    {
      "handle": "filesystem"
    }
  ]
}
