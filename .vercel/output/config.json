{
  "version": 3,
  "routes": [
    {
      "src": "^/_astro/(.*)$",
      "mode": "no-cors",
      "headers": {
        "content-type": "application/json charset=utf-8",
        "Access-Control-Allow-Origin": "*"
      },
      "continue": true
    },
    {
      "handle": "filesystem"
    }
  ]
}
