{
  "version": 3,
  "routes": [
    {
      "src": "^/_astro/(.*)$",
      "mode": "no-cors",
      "headers": {
        "access-control-allow-origin": "*",
        "cache-control": "public, max-age=31536000, immutable",
        "content-type": "application/json charset=utf-8"
      },
      "continue": true
    },
    {
      "handle": "filesystem"
    }
  ]
}
